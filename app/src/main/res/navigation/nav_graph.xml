<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/titleScreen">



    <fragment
        android:id="@+id/titleScreen"
        android:name="su.cus.announce.TitleScreen"
        android:label="TitleScreen"
        tools:layout="@layout/title_screen">
        <action
            android:id="@+id/action_titleScreen_to_premiereList"
            app:destination="@id/premiereList" />
    </fragment>
    <fragment
        android:id="@+id/premiereList"
        android:name="su.cus.announce.premiere.PremiereList"
        android:label="list_premiere"
        tools:layout="@layout/list_premiere" >
        <action
            android:id="@+id/action_premiereList_to_descriptionFragment"
            app:destination="@id/descriptionFragment" />
    </fragment>
    <fragment
        android:id="@+id/descriptionFragment"
        android:name="su.cus.announce.DescriptionFragment"
        android:label="DescriptionFragment"
        tools:layout="@layout/fragment_description">
        <argument
            android:name="filmDataItem"
            app:argType="su.cus.announce.API.FilmDescription.FilmDataItem" />
        <action
            android:id="@+id/action_descriptionFragment_to_premiereList"
            app:destination="@id/premiereList" />
    </fragment>

    <fragment
        android:id="@+id/FirstFragment"
        android:name="su.cus.announce.DescriptionActivity.FirstFragment"
        android:label="@string/first_fragment_label"
        tools:layout="@layout/fragment_first">

        <action
            android:id="@+id/action_FirstFragment_to_SecondFragment"
            app:destination="@id/SecondFragment" />
    </fragment>
    <fragment
        android:id="@+id/SecondFragment"
        android:name="su.cus.announce.DescriptionActivity.SecondFragment"
        android:label="@string/second_fragment_label"
        tools:layout="@layout/fragment_second">

        <action
            android:id="@+id/action_SecondFragment_to_FirstFragment"
            app:destination="@id/FirstFragment" />
    </fragment>



</navigation>